{
    "_version": "1.4.0",
    "sap.app": {
        "id": "eut.eutobjectsrv",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": {
            "version": "0.0.1"
        },
        "title": "{{appTitle}}",
        "description": "{{appDescription}}",
        "resources": "resources.json",
        "sourceTemplate": {
            "id": "@sap-ux/fiori-elements-writer:lrop",
            "version": "0.3.6",
            "toolsId": "80cdc1aa-4dd5-4f41-871b-34ad9f125429"
        },
        "dataSources": {
            "mainService": {
                "uri": "/eutobject/",
                "type": "OData",
                "settings": {
                    "annotations": [
                        "annotation"
                    ],
                    "localUri": "localService/metadata.xml",
                    "odataVersion": "4.0"
                }
            },
            "annotation": {
                "type": "ODataAnnotation",
                "uri": "annotations/annotation.xml",
                "settings": {
                    "localUri": "annotations/annotation.xml"
                }
            }
        }
    },
    "sap.ui": {
        "technology": "UI5",
        "icons": {
            "icon": "",
            "favIcon": "",
            "phone": "",
            "phone@2": "",
            "tablet": "",
            "tablet@2": ""
        },
        "deviceTypes": {
            "desktop": true,
            "tablet": true,
            "phone": true
        }
    },
    "sap.ui5": {
        "flexEnabled": true,
        "dependencies": {
            "minUI5Version": "1.102.1",
            "libs": {
                "sap.m": {},
                "sap.ui.core": {},
                "sap.ushell": {},
                "sap.fe.templates": {},
                "sap.f": {}
            }
        },
        "contentDensities": {
            "compact": true,
            "cozy": true
        },
        "models": {
            "i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "settings": {
                    "bundleName": "eut.eutobjectsrv.i18n.i18n"
                }
            },
            "": {
                "dataSource": "mainService",
                "preload": true,
                "settings": {
                    "synchronizationMode": "None",
                    "operationMode": "Server",
                    "autoExpandSelect": true,
                    "earlyRequests": true
                }
            },
            "@i18n": {
                "type": "sap.ui.model.resource.ResourceModel",
                "uri": "i18n/i18n.properties"
            }
        },
        "resources": {
            "css": []
        },
        "routing": {
            "config": {
                "flexibleColumnLayout": {
                    "defaultTwoColumnLayoutType": "TwoColumnsMidExpanded",
                    "defaultThreeColumnLayoutType": "ThreeColumnsEndExpanded"
                },
                "routerClass": "sap.f.routing.Router"
            },
            "routes": [
                {
                    "pattern": ":?query:",
                    "name": "EUTObjectList",
                    "target": [
                        "EUTObjectList"
                    ]
                },
                {
                    "pattern": "EUTObject({key}):?query:",
                    "name": "EUTObjectObjectPage",
                    "target": [
                        "EUTObjectList",
                        "EUTObjectObjectPage"
                    ]
                },
                {
                    "pattern": "EUTObject({key})/Activities({key2}):?query:",
                    "name": "EUT_ActivitiesObjectPage",
                    "target": [
                        "EUTObjectList",
                        "EUTObjectObjectPage",
                        "EUT_ActivitiesObjectPage"
                    ]
                },
                {
                    "pattern": "EUTObject({key})/Activities({key2})/Financial_Input({key3}):?query:",
                    "name": "FINANCIAL_INPUTObjectPage",
                    "target": [
                        "FINANCIAL_INPUTObjectPage"
                    ]
                },
                {
                    "pattern": "EUTObject({key})/Activities({key2})/Screening_Input({key3}):?query:",
                    "name": "EUT_SCREENING_INPUTObjectPage",
                    "target": [
                        "EUT_SCREENING_INPUTObjectPage"
                    ]
                }
            ],
            "targets": {
                "EUTObjectList": {
                    "type": "Component",
                    "id": "EUTObjectList",
                    "name": "sap.fe.templates.ListReport",
                    "options": {
                        "settings": {
                            "entitySet": "EUTObject",
                            "variantManagement": "Page",
                            "hideFilterBar": false,
                            "navigation": {
                                "EUTObject": {
                                    "detail": {
                                        "route": "EUTObjectObjectPage"
                                    }
                                }
                            },
                            "initialLoad": "Enabled",
                            "controlConfiguration": {
                                "@com.sap.vocabularies.UI.v1.LineItem": {
                                    "actions": {
                                        "importData": {
                                            "id": "importDataButton",
                                            "text": "Import Data",
                                            "press": "eut.eutobjectsrv.controller.ListReportExtController.dataImport",
                                            "requiresSelection": false
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "beginColumnPages",
                    "contextPattern": ""
                },
                "EUTObjectObjectPage": {
                    "type": "Component",
                    "id": "EUTObjectObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "showRelatedApps": true,   
                            "sectionLayout": "Tabs",
                            "entitySet": "EUTObject",
                            "navigation": {
                                "Activities": {
                                    "detail": {
                                        "route": "EUT_ActivitiesObjectPage"
                                    }
                                }
                            },
                            "content":{
                                "header": {
                                    "editableHeaderContent": false,
                                    "actions": {
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "Activities/@com.sap.vocabularies.UI.v1.LineItem#Activities": {
                                    "tableSettings": {
                                        "enableMassEdit": false,
                                        "personalization": {
                                            "sort": true,
                                            "filter": true,
                                            "column": true,
                                            "group": true
                                        },
                                        "type": "ResponsiveTable",
                                        "selectionMode": "Multi",
                                        "enableExport": true
                                    },
                                     "actions": {
                                          "selectEntries": {
                                            "visible": true,
                                            "enabled": true,
                                            "text": "Select Economic Activity(s)",
                                            "press": "eut.eutobjectsrv.controller.ListReportExtController.selectEAs"
                                          }
                                     }
                                }
                            }                            
                        }
                    },
                    "controlAggregation": "midColumnPages",
                    "contextPattern": "/EUTObject({key})"
                },
                "EUT_ActivitiesObjectPage": {
                    "type": "Component",
                    "id": "EUT_ActivitiesObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "editableHeaderContent": true,
                            "entitySet": "EUT_Activities",
                            "sectionLayout": "Tabs",
                            "variantManagement":"Control",
                            "navigation": {
                                "Financial_Input": {
                                    "detail": {
                                        "route": "FINANCIAL_INPUTObjectPage"
                                    }
                                },
                                "Screening_Input": {
                                    "detail": {
                                        "route": "EUT_SCREENING_INPUTObjectPage"
                                    }
                                }
                            },
                            "controlConfiguration": {
                                "Financial_Input/@com.sap.vocabularies.UI.v1.LineItem#FinInputs": {
                                    "tableSettings": {
                                        "enableMassEdit": false,
                                        "personalization": {
                                            "sort": true,
                                            "filter": true,
                                            "column": true,
                                            "group": true
                                        },
                                        "type": "ResponsiveTable",
                                        "selectionMode": "Multi",
                                        "enableExport": true
                                    },
                                     "actions": {
                                          "selectEntries": {
                                            "visible": true,
                                            "enabled": true,
                                            "text": "Select Financial Record(s)",
                                            "press": "eut.eutobjectsrv.controller.ListReportExtController.selectFRs"
                                          }
                                     }
                                },
                                "Screening_Input/@com.sap.vocabularies.UI.v1.LineItem#ScreenInputs": {
                                    "tableSettings": {
                                        "enableMassEdit": true,
                                        "quickVariantSelection": {
                                            "paths": [
                                                {
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.PresentationVariant"
                                                },
                                                {
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#With"
                                                },
                                                {
                                                    "annotationPath": "com.sap.vocabularies.UI.v1.SelectionVariant#Without"
                                                }                                              
                                            ]
                                        }
                                    },
                                    "actions": {
                                        "importData": {
                                            "id": "insertInputValueButton",
                                            "text": "Select Screening Inputs",
                                            "press": "ns.worklist.controller.ListReportExtController.insertInput",
                                            "requiresSelection": false
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "controlAggregation": "endColumnPages",
                    "contextPattern": "/EUTObject({key})/Activities({key2})"
                },
                "FINANCIAL_INPUTObjectPage": {
                    "type": "Component",
                    "id": "FINANCIAL_INPUTObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "FINANCIAL_INPUT",
                            "navigation": {},
                            "content": {
                                "header": {
                                    "visible": true
                                }
                            }
                        }
                    },
                    "controlAggregation": "endColumnPages",
                    "contextPattern": "/EUTObject({key})/Activities({key2})/Financial_Input({key3})"
                },
                "EUT_SCREENING_INPUTObjectPage": {
                    "type": "Component",
                    "id": "EUT_SCREENING_INPUTObjectPage",
                    "name": "sap.fe.templates.ObjectPage",
                    "options": {
                        "settings": {
                            "entitySet": "EUT_SCREENING_INPUT",
                            "navigation": {},
                            "content": {
                                "header": {
                                    "visible": true
                                }
                            }
                        }
                    },
                    "controlAggregation": "endColumnPages",
                    "contextPattern": "/EUTObject({key})/Activities({key2})/Screening_Input({key3})"
                }
            }
        },
        "rootView": {
            "viewName": "sap.fe.templates.RootContainer.view.Fcl",
            "type": "XML",
            "async": true,
            "id": "appRootView"
        }
    },
    "sap.fiori": {
        "registrationIds": [],
        "archeType": "transactional"
    }
}